{
  "name": "@straw-hat/openapi-web-sdk-generator",
  "version": "1.0.0",
  "description": "A Web SDK Generator using OpenAPI v3.",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/openapi-web-sdk-generator.git"
  },
  "license": "MIT",
  "keywords": [
    "openapi",
    "generator",
    "fetch",
    "http",
    "sdk",
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "test": "jest",
    "prepack": "yarn build && yarn oclif:manifest && yarn oclif:readme",
    "postpack": "rimraf oclif.manifest.json",
    "oclif:manifest": "oclif-dev manifest",
    "oclif:readme": "oclif-dev readme --multi --dir=docs/commands"
  },
  "bin": {
    "sht-openapi-web-sdk-generator": "bin/sht-openapi-web-sdk-generator"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "README.md",
    "templates",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3.2.1",
    "@straw-hat/fancy-map": "^1.0.2",
    "@straw-hat/prettier-config": "^1.2.0",
    "change-case": "^4.1.2",
    "debug": "^4.3.1",
    "del": "^6.0.0",
    "ejs": "^3.1.5",
    "make-dir": "^3.1.0",
    "openapi-types": "^7.0.1",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@straw-hat/tsconfig": "^2.1.1",
    "@types/debug": "^4.1.5",
    "@types/ejs": "^3.0.5",
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "sht-openapi-web-sdk-generator",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
